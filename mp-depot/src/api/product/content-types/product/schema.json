{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "cover": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-cover.product-cover"
    },
    "pattern": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-pattern.product-pattern"
    },
    "pages": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-page.product-page"
    },
    "ruling": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-ruling.product-ruling"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
